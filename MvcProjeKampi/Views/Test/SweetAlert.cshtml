
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src=" http://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<button class="btn btn-info" id="btn1">Button1</button>
<button class="btn btn-primary" id="btn2">Button2</button>
<button class="btn btn-success" id="btn3">Button</button>
<script src="~/Scripts/jquery-3.4.1.js"></script>

<script>
    $('#btn1').click(function () {
        swal("Merhaba Mvc Proje Kampı!");
    });
</script>

<script>
    $('#btn2').click(function () {
        swal("Mvc Proje Kampı!", "...Ders96!");
    });
</script>
<script>
    $('#btn3').click(function () {
        swal("Tebrikler!", "Başarılı Bir Şekilde işlme gerçekleştirildi", "success");
    });
</script>
<script src="~/AdminLTE-3.0.4/plugins/jquery/jquery.min.js"></script>